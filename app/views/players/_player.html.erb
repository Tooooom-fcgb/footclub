<div id="<%= dom_id player %>">
  <p>
    <strong>Photo:</strong>
    <%= player.photo %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= player.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= player.surname %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= player.age %>
  </p>

  <p>
    <strong>Club:</strong>
    <%= player.club_id %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= player.position %>
  </p>

</div>
